	<queryString>
		<![CDATA[SELECT
     sum(pago_servicio.valor_servicio) AS Total,
     usuarios.`Nombres` AS Nombre,
     usuarios.`Apellidos` AS Apellido,
     pago_servicio.`Fecha_Pago_Barbero` AS Fecha
FROM
     `usuarios` usuarios INNER JOIN `pago_servicio` pago_servicio ON usuarios.`idUsuarios` = pago_servicio.`usuarios_idUsuarios`
WHERE
     pago_servicio.`Fecha_Pago_Barbero` between $P{fechaInicial} and $P{fechaFinal}
GROUP BY
     pago_servicio.usuarios_idUsuarios]]>
	</queryString>
